<div id="Box_form">
<%= form_for [:admin, @line], :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
<br />
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
<br />
  <%= f.file_field :imgline %> 
  <p>
<%= submit_tag "Voltar",:type=>"button",:onclick=>"window.open('"+admin_lines_path+"','_self');", :class => "Bt_enviar", :style=>"margin-right: 5px;"%> <%= f.submit "Salvar", :class => "Bt_enviar" %>

</p>
<% end %>
</div>
